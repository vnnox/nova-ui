{
  "name": "@vnnox/novaui",
  "version": "1.5.1",
  "description": "nova ui components",
  "main": "libs/nova.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "rm -rf docs/* && webpack-dev-server --env=development  --mode=development  --config scripts/dev.webpack.js",
    "doc": "rm -rf docs/assets/* && webpack --env=production --mode=production --config scripts/dev.webpack.js",
    "build": "webpack --env=production --mode=production --config scripts/build.webpack.js && webpack --env=production --mode=production --config scripts/vue.webpack.js",
    "stylelint": "stylelint src/**/*.scss examples/**/*.scss",
    "stylelint-fix": "stylelint --fix src/**/*.scss examples/**/*.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vnnox/nova-ui.git"
  },
  "keywords": [
    "nova",
    "vnnox",
    "ui",
    "components"
  ],
  "author": "smohan <https://smohan.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vnnox/nova-ui/issues"
  },
  "homepage": "https://github.com/vnnox/nova-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.2",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "markdown-it-container": "^2.0.0",
    "markdownit-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "npm": "^6.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-scss": "^3.3.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.3.1"
  }
